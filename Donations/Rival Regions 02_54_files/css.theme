#index_region_show_over{
	background: -moz-linear-gradient(left, #292929 0%, rgba(0,0,0,0) 100%); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, right top, color-stop(0%,#292929), color-stop(100%,rgba(0,0,0,0))); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(left, #292929 0%,rgba(0,0,0,0) 100%); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(left, #292929 0%,rgba(0,0,0,0) 100%); /* Opera 11.10+ */
	background: -ms-linear-gradient(left, #292929 0%,rgba(0,0,0,0) 100%); /* IE10+ */
	background: linear-gradient(to right, #292929 0%,rgba(0,0,0,0) 100%); /* W3C */
}
	
