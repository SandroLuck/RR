.guide_show{
	position: absolute;
	margin-left: 50%;
	line-height: 32px;
	padding: 3px;
	border-radius: 3px;
	display: none;
	color: #f1f1f1;
	font-size: 18px;
	font-family: Impact;
	left: 50%;
	background-color: rgba(0,0,0,0.4);
}
.guide_show_text{
	padding-left: 7px; padding-top: 0px; padding-right: 4px;
}

.header_buttons_hover:hover, .hov:hover{
	background-color: #3c6ae0;
}

.hov{
	cursor: pointer;
}

.header_buttons_hover_green:hover{
